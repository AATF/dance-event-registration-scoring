<% @title = "Edit Setting" %>
<% @header = "Edit Setting" %>

<%= form_for @setting, url: admin_setting_path(@setting.var) do |s| %>
  <div class="form-group row">
    <%= s.label :var, "Name", class: "col-sm-2 col-form-label" %>
    <div class="col-sm-10">
      <%= s.text_field :value, class: "form-control" %>
    </div>
  </div>
<% end %>