<% @title = "New Setting" %>
<% @header = "Add New Setting" %>

<%= form_for @setting, class: "form-control" do |s| %>
  <%= s.label :val, "Name" %>
  <%= s.text_field :var, class: "form-control " %>

  <%= s.label :value, "Value" %>
  <%= s.text_field :value, class: "form-control" %>

  <%= s.button "Submit", class: "btn btn-primary" %>
<% end %>
