<% @title = "Edit Dancer" %>
<% @header = "Edit Dancer #{@dancer.name}" %>

<%= form_for @dancer, html: { class: "form-control" } do |f| %>
    <%= f.label :name, "Name" %>
    <%= f.text_field :name, class: "form-control" %>

    <%= f.label :dance_name, "Dance Name" %>
    <%= f.text_field :dance_name, class: "form-control" %>

    <%= f.button "Submit", class: "btn btn-primary" %>
<% end %>
